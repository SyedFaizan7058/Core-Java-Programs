package com.numbers;

import java.util.function.Supplier;

public class FibbonancySeries

{
	public static void main(String[] args) {

		Supplier<Integer> f = () -> {

			int f1 = 0, f2 = 1, f3 = 0;
			int n = 10;

			for (int i = 0; i < n; i++) {
				f3 = f1 + f2;
				f1 = f2;
				f2 = f3;

			}

			return f3;

		};

		int n = 10;

		for (int i = 0; i < n; i++) {

			System.out.print(f.get() + " ");
		}

	}
}
