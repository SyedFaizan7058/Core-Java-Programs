package com.overload.time;

public class Time {

	private int hours;
	private int minutes;
	private int seconds;

	public Time(int hours) {

		this.hours = hours;
	}

	public Time(int hours, int minutes) {

		this.hours = hours;
		this.minutes = minutes;
	}

	public Time(int hours, int minutes, int seconds) {

		this.hours = hours;
		this.minutes = minutes;
		this.minutes=minutes;
	}

}
